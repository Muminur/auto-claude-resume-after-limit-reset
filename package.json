{
  "name": "auto-resume",
  "version": "1.2.1",
  "description": "Automatically resume Claude Code terminal sessions when rate limits reset",
  "main": "auto-resume-daemon.js",
  "bin": {
    "auto-resume-daemon": "./auto-resume-daemon.js"
  },
  "scripts": {
    "postinstall": "node scripts/setup-daemon.js",
    "daemon:start": "node auto-resume-daemon.js start",
    "daemon:stop": "node auto-resume-daemon.js stop",
    "daemon:status": "node auto-resume-daemon.js status",
    "daemon:restart": "node auto-resume-daemon.js restart",
    "test": "node test-daemon.js"
  },
  "keywords": [
    "claude",
    "claude-code",
    "claude-plugin",
    "auto-resume",
    "rate-limit",
    "cli"
  ],
  "author": "Muminur",
  "license": "MIT",
  "dependencies": {
    "clipboardy": "^4.0.0"
  },
  "optionalDependencies": {
    "robotjs": "^0.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
